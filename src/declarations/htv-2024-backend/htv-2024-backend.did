type PostSnapshot = 
 record {
   creator: text;
   description: text;
   funding_amount: nat;
   id: nat;
   images: vec blob;
   likes: nat;
   title: text;
 };
service : {
  createPost: (vec blob, text, text, text) -> ();
  getAllPostSnapshots: () -> (vec PostSnapshot) query;
  getPostSnapshot: (nat) -> (PostSnapshot) query;
  getTopPostSnapshots: () -> (vec PostSnapshot) query;
  likePost: (nat) -> ();
}
